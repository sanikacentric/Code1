public class reverseIntegerss13 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
public static int reverseInteger(int x) {
	int res =0;
	while(x!=0) {
		res = res*10+x%10;
		x=x/10;
	}
	if(res<Integer.MIN_VALUE||res>Integer.MAX_VALUE) {
		return -1;
	}
	return res;
}
}
